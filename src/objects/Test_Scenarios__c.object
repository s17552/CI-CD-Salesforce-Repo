    <validationRules>
        <fullName>Defect_Coment_required_if_Failed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Result__c, &apos;FAIL&apos;), 
ISBLANK(Defect_Comments__c))</errorConditionFormula>
        <errorMessage>Please enter Defect Comments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Result_Change_On_Case_E7_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Result__c),
ISPICKVAL(Result__c,&apos;Pass&apos;), 
RecordType.DeveloperName = &apos;UAT_Test_Scenario&apos;,
NOT(ISPICKVAL(Case__r.Status,&apos;E7 - UAT&apos;)),
AND(
NOT(ISPICKVAL(Case__r.Application__c ,&apos;AEM&apos;)),
NOT(ISPICKVAL(Case__r.Application__c ,&apos;Resource Center&apos;)),
NOT(ISPICKVAL(Case__r.Application__c,&apos;JDE&apos;)),
Case__r.RecordType.Name = &quot;Application Enhancement&quot;,
NOT(ISPICKVAL(Case__r.Application__c ,&apos;ITS Other&apos;))
)
)</errorConditionFormula>
        <errorMessage>Test scenario with record type UAT can have results changed only in status E7 - UAT</errorMessage>
    </validationRules>
