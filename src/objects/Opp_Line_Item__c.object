    <fields>
        <fullName>Timing__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
TEXT(Product__r.Interval__c) = &apos;Perpetual&apos;, 
&quot;Perpetual&quot;, 

IF( 
TEXT(Product__r.Interval__c) = &apos;Negotiated&apos;, 
&quot;Negotiated&quot;, 

IF( 
TEXT(Product__r.Interval__c) = &apos;One-Time&apos;, 
&quot;One-Time&quot;, 

IF( 
TEXT(Product__r.Interval__c) = &apos;Project&apos;, 
&quot;Project&quot;, 

IF( 
TEXT(Product__r.Interval__c) = &apos;Fixed&apos;, 
&quot;Fixed&quot;, 

IF( 
TEXT(Product__r.Interval__c) = &apos;Fee&apos;, 
&quot;Fee&quot;, 

IF( 
TEXT(Product__r.Interval__c) = &apos;Package&apos;, 
&quot;Package&quot;, 

IF( 
AND(TEXT(Product__r.Interval__c) = &apos;Annual&apos;, ISNULL(Start_Date__c), ISNULL(End_Date__c)), 
&quot;Annual (Missing Start and End Dates)&quot;, 


IF( 
AND(TEXT(Product__r.Interval__c) = &apos;Term&apos;, ISNULL(Start_Date__c), ISNULL(End_Date__c)), 
&quot;Term(Missing Start and End Dates)&quot;, 

IF( 
AND(TEXT(Product__r.Interval__c) = &apos;Subscription&apos;, ISNULL(Start_Date__c), ISNULL(End_Date__c)), 
&quot;Subscription(Missing Start and End Dates)&quot;, 

IF( 
AND( TEXT(Product__r.Interval__c) = &apos;Annual&apos;, NOT(ISNULL(Start_Date__c)), NOT(ISNULL(End_Date__c))), 
&quot;Annual (&quot; &amp; ( CASE( MONTH( Start_Date__c) , 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
9, &quot;Sep&quot;, 
10, &quot;Oct&quot;, 
11, &quot;Nov&quot;, 
12, &quot;Dec&quot;, 
&quot;None&quot;) &amp; &quot;/&quot; &amp; TEXT(DAY(Start_Date__c)) &amp; &quot;/&quot; &amp; TEXT(YEAR(Start_Date__c))) &amp; &quot; - &quot; &amp; ( CASE( MONTH( End_Date__c) , 
1, &quot;Jan&quot;, 
2, &quot;Feb&quot;, 
3, &quot;Mar&quot;, 
4, &quot;Apr&quot;, 
5, &quot;May&quot;, 
6, &quot;Jun&quot;, 
7, &quot;Jul&quot;, 
8, &quot;Aug&quot;, 
