        <description>Displays primary quote of related opportunity or displays legal document</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( Legal_Documents__c ),
  IF(ISBLANK(  Governing_Document__c ),
  If(ISBLANK( Related_Opportunity__c) ,
  &quot;Missing&quot;,
     If(ISBLANK(Related_Opportunity__r.SBQQ__PrimaryQuote__c),
        &quot;No Primary Quote&quot;,
        HYPERLINK(&quot;/&quot;&amp;  Related_Opportunity__r.SBQQ__PrimaryQuote__c ,
        Related_Opportunity__r.SBQQ__PrimaryQuote__r.Name ))
  ),
IF( Governing_Document__r.RecordType.DeveloperName=&apos;Legacy&apos;,HYPERLINK( Governing_Document__r.LDContractLink__c , Governing_Document__r.Name ),
HYPERLINK(&quot;/&quot;&amp;Governing_Document__c, Governing_Document__r.Name ))),
HYPERLINK(&quot;/&quot;&amp; Legal_Documents__c, Legal_Documents__r.Name ))</formula>
        <label>Contract</label>
        <required>false</required>
