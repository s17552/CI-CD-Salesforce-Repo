    <validationRules>
        <fullName>No_Opportunity_or_Legal_Document</fullName>
        <active>true</active>
        <description>User must select either a Legal Document or Related Opportunity.</description>
        <errorConditionFormula>AND(
  ISBLANK( Related_Opportunity__c ),
  ISBLANK(  Legal_Documents__c ),
  ISBLANK(  Governing_Document__c )
)</errorConditionFormula>
        <errorMessage>You must relate either a Legal Document or Related Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_Opportunity_or_Legal_Document</fullName>
        <active>true</active>
        <description>User should only be able to select one</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Related_Opportunity__c)),
OR(
  NOT(ISBLANK(Legal_Documents__c)),
  NOT(ISBLANK( Governing_Document__c ))
)
)</errorConditionFormula>
        <errorMessage>You cannot select both a related opportunity and related legal document. Please select only one.</errorMessage>
    </validationRules>
