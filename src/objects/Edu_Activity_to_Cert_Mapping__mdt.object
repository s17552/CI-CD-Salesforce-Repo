<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Used to identify which edu activities map directly to what certifications</description>
    <fields>
        <fullName>CSOD_ID__c</fullName>
        <description>Cornerstone ID of the event,session,or test that can grant certifications</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Cornerstone ID of the event,session,or test that can grant certifications</inlineHelpText>
        <label>CSOD_ID</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Cert_Name__c</fullName>
        <description>long text version of cert name</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Cert Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Cert_Type__c</fullName>
        <description>Determines which cert type when querying</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Cert Type</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Analyst</fullName>
                    <default>true</default>
                    <label>Analyst</label>
                </value>
                <value>
                    <fullName>Architect</fullName>
                    <default>false</default>
                    <label>Architect</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>EDU_Activity_Name__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>EDU Activity Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Name_abbv__c</fullName>
        <description>short abbreviation for cert name</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Name abbv</label>
        <length>55</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>View_More_Link__c</fullName>
        <description>link to navigate to the microstrategy corporate site to find out information about the certification</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>link to navigate to the microstrategy corporate site to find out information about the certification</inlineHelpText>
        <label>View More Link</label>
        <required>false</required>
        <type>Url</type>
    </fields>
    <label>Edu Activity to Cert Mapping</label>
    <pluralLabel>Edu Activity to Cert Mappings</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
