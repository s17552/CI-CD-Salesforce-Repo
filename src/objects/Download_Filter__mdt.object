        <length>10</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>isLatestReleaseType__c</fullName>
        <defaultValue>false</defaultValue>
        <description>checkbox for latest releasetype concat with Version meant to assist in default selections</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>isLatestReleaseType</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>is_RC__c</fullName>
        <defaultValue>false</defaultValue>
