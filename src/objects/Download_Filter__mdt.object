                </value>
                <value>
                    <fullName>RC 2021</fullName>
                    <default>false</default>
                    <label>RC 2021</label>
                </value>
                <value>
                    <fullName>Identity 2021</fullName>
                    <default>false</default>
                    <label>Identity 2021</label>
                </value>
                <value>
                    <fullName>HyperWeb CRX</fullName>
                    <default>false</default>
                    <label>HyperWeb CRX</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Is_Eligible__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Is Eligible</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>VersionDetail__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
