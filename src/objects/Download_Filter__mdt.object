        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>is RC</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>osGroupList__c</fullName>
        <description>semi-colon delimited os groups based on version number</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
