        <fullName>Prevent_Approval_Process_Rejection</fullName>
        <active>true</active>
        <description>Prevent CXO Rejection during Feature Process</description>
        <errorConditionFormula>Prevent_Rejection__c</errorConditionFormula>
        <errorMessage>Feature Objects cannot have their Approvals Rejected. Please rate your Value and Tempo and approve the recordl</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_from_edits_when_stage_not_F1</fullName>
        <active>true</active>
        <description>Prevent from editing the record when stage is different than F1</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(Status__c,&apos;F2 - CXO&apos;),
ISPICKVAL(Status__c,&apos;F3 - CEO&apos;),
ISPICKVAL(Status__c,&apos;F4 - APV&apos;)),
OR(
ISCHANGED(OwnerId),
ISCHANGED(Days__c),
ISCHANGED(Confidence__c),
ISCHANGED(Percentage_Done__c),
ISCHANGED(Affected_Customers__c),
ISCHANGED(InvestmentCategory__c),
ISCHANGED(Product__c),
ISCHANGED(UC_Capability__c),
ISCHANGED(Lead_Team__c) ,
ISCHANGED(State__c),
ISCHANGED(Release__c),  
ISCHANGED(Release_Date__c) 
)
)</errorConditionFormula>
        <errorMessage>You can&apos;t edit the record if in F2, F3, or F4 status.</errorMessage>
    </validationRules>
