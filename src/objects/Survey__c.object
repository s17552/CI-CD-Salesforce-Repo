    <fields>
        <fullName>Score__c</fullName>
        <externalId>false</externalId>
        <formula>(IF(
   ISPICKVAL(Answers_1__c, &quot;Delighted&quot;), 1,
   IF(
      ISPICKVAL(Answers_1__c, &quot;Very Satisfied&quot;), 0.75,
      IF(
         ISPICKVAL(Answers_1__c, &quot;Satisfied&quot;), 0.50,
         IF(
            ISPICKVAL(Answers_1__c, &quot;Partially Satisfied&quot;), 0.25, 0))))
+
IF(
   ISPICKVAL(Answers_2__c, &quot;Delighted&quot;), 1,
   IF(
      ISPICKVAL(Answers_2__c, &quot;Very Satisfied&quot;), 0.75,
      IF(
         ISPICKVAL(Answers_2__c, &quot;Satisfied&quot;), 0.50,
         IF(
            ISPICKVAL(Answers_2__c, &quot;Partially Satisfied&quot;), 0.25, 0))))
+
IF(
   ISPICKVAL(Answers_3__c, &quot;Delighted&quot;), 1,
   IF(
      ISPICKVAL(Answers_3__c, &quot;Very Satisfied&quot;), 0.75,
      IF(
         ISPICKVAL(Answers_3__c, &quot;Satisfied&quot;), 0.50,
         IF(
            ISPICKVAL(Answers_3__c, &quot;Partially Satisfied&quot;), 0.25, 0)))))
            
/

IF(
   AND(
       ISBLANK(TEXT(Answers_1__c)),
       ISBLANK(TEXT(Answers_2__c)),
       ISBLANK(TEXT(Answers_3__c))
      ),
Null,
IF(
   ISBLANK(TEXT(Answers_1__c)),
   0,
   1
  )
+
IF(
   ISBLANK(TEXT(Answers_2__c)),
   0,
   1
  )
+
IF(
   ISBLANK(TEXT(Answers_3__c)),
   0,
   1
  ))</formula>
        <label>Satisfaction</label>
