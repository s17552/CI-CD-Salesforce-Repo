    <fields>
        <fullName>Created_Period_Reference__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
  OR(
    AND(
      YEAR(DATEVALUE(CreatedDate)) = 2016,
      DATEVALUE(CreatedDate) &lt;= DATE(2016,5,7)),
    AND(
      YEAR(DATEVALUE(CreatedDate)) = 2017,
      DATEVALUE(CreatedDate) &lt;= DATE(2017,5,6))),
TEXT(0),
NULL)</formula>
        <label>Created Period Reference</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Created_Period__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
DATEVALUE(CreatedDate) = DATE(2017,12,31),
&quot;FY17 SP-26&quot;,
IF(
YEAR(DATEVALUE(CreatedDate)) = 2016,
&quot;FY16 SP-&quot; &amp; Created_Period_Reference__c 
&amp;
TEXT(
    CEILING( 
          (MOD(
               FLOOR(
                     (DATEVALUE(CreatedDate) - DATE(2016,01,03)) / 7
                    ),
               52
          ) + 1) / 2
         )
),
IF(
YEAR(DATEVALUE(CreatedDate)) = 2017,
&quot;FY17 SP-&quot; &amp; Created_Period_Reference__c
&amp;
TEXT(
    CEILING( 
          (MOD(
               FLOOR(
                     (DATEVALUE(CreatedDate) - DATE(2017,01,01)) / 7
                    ),
               52
          ) + 1) / 2
         )

),NULL)))</formula>
        <label>Created Period</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
