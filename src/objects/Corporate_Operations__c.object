    <validationRules>
        <fullName>Prevent_Updates_at_Sales_Period_Closing</fullName>
        <active>false</active>
        <description>Record cannot be edited after the Sales Period is Closed</description>
        <errorConditionFormula>AND
(
NOT(ISNEW()),
OR(
ISCHANGED(Program_Name__c),
ISCHANGED(Sales_Period__c),
ISCHANGED(Status__c),
ISCHANGED(Initiatives__c),
ISCHANGED(SalesPeriodKeyObjectivesResults__c),
ISCHANGED(Positive_Achievements_Highlights__c),
ISCHANGED(Challenges_and_Setbacks_Lowlights__c),
ISCHANGED(Status_Versus_12_Weeks_Ago__c),
ISCHANGED(Key_Objectives_Next_Period__c),
ISCHANGED(Confidence_level__c),
ISCHANGED(Request_for_Management_Support__c)
),
TODAY() - Sales_Period__r.End_Date__c &gt;=  12,
$User.EmployeeNumber &lt;&gt; &apos;15308&apos;
)</errorConditionFormula>
        <errorMessage>This Periodic Report Form cannot be updated after the Sales Period is Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Period_cannot_be_updated</fullName>
        <active>true</active>
        <description>Quarter cannot be updated after record is created.</description>
        <errorConditionFormula>AND 
( 
NOT(ISNEW()), 
ISCHANGED(Quarter__c),
$User.EmployeeNumber &lt;&gt;&apos;13897&apos;, 
$User.EmployeeNumber &lt;&gt;&apos;13353&apos;, 
$User.EmployeeNumber &lt;&gt;&apos;14169&apos;
)</errorConditionFormula>
        <errorMessage>Quarter cannot be updated after the record is created.</errorMessage>
    </validationRules>
