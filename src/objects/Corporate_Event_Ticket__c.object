    <validationRules>
        <fullName>Event_Explanation_Required</fullName>
        <active>true</active>
        <description>Event explanation comments are required if event type are Customer Event or Team Event.</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(Event_Type__c,&apos;Customer Event&apos;),
ISPICKVAL(Event_Type__c,&apos;Team Event&apos;)
),
ISBLANK( Event_Explanation__c )
)</errorConditionFormula>
        <errorMessage>Event Explanation is required if the event type is Customer or Team Event.</errorMessage>
    </validationRules>
